  "InLieuOfSuffix": ")",
  "WorkingDays": "{ 2, 3, 4, 5, 6 }",
  "HolidaysDefinitionTable": "HolidaysDefinition",

  "HolidaysReference": {
    "TableName": "Holidays",
    "DateColumnName": "Holiday Date",
    "HolidayColumnName": "Holiday Name"
